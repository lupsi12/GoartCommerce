{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/products",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5233
        }
      ],
      "UpstreamPathTemplate": "/products",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ]
    },
    {
      "DownstreamPathTemplate": "/api/carts",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5259
        }
      ],
      "UpstreamPathTemplate": "/carts",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:5000"
  },
  "SwaggerEndPoints": [
    {
      "Name": "Products API",
      "Version": "v1",
      "Url": "http://localhost:5233/swagger/v1/swagger.json"
    },
    {
      "Name": "Carts API",
      "Version": "v1",
      "Url": "http://localhost:5259/swagger/v1/swagger.json"
    }
  ]
}
