  "SwaggerEndPoints": [
    {
      "Key": "CartService",
      "Config": {
        "Name": "Cart Service",
        "Version": "v1",
        "Url": "http://localhost:5259/swagger/v1/swagger.json"
      }
    },
    {
      "Key": "ProductService",
      "Config": {
        "Name": "Product Service",
        "Version": "v1",
        "Url": "http://localhost:5233/swagger/v1/swagger.json"
      }
    },
    {
      "Key": "AuthService",
      "Config": {
        "Name": "Auth Service",
        "Version": "v1",
        "Url": "http://localhost:5216/swagger/v1/swagger.json"
      }
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5111"
  }
}
