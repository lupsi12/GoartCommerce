{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/cart/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5259
        }
      ],
      "UpstreamPathTemplate": "/cart-gate/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "SwaggerKey": "CartService"
    },
    {
      "DownstreamPathTemplate": "/api/products/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5233
        }
      ],
      "UpstreamPathTemplate": "/product-gate/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "SwaggerKey": "ProductService"
    },
    {
      "DownstreamPathTemplate": "/api/auth/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5216
        }
      ],
      "UpstreamPathTemplate": "/auth-gate/{everything}",
      "UpstreamHttpMethod": [ "POST" ],
      "SwaggerKey": "AuthService"
    },
    {
      "DownstreamPathTemplate": "/api/users/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5216
        }
      ],
      "UpstreamPathTemplate": "/user-gate/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "SwaggerKey": "AuthService"
    }
