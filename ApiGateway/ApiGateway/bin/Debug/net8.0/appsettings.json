{
  "ReverseProxy": {
    "Routes": {
      "cartRoute": {
        "ClusterId": "cartCluster",
        "Match": {
          "Path": "/cart-gate/{**catch-all}",
          "Methods": [ "GET", "POST", "PUT", "DELETE" ]
        }
      },
      "productRoute": {
        "ClusterId": "productCluster",
        "Match": {
          "Path": "/product-gate/{**catch-all}",
          "Methods": [ "GET", "POST", "PUT", "DELETE" ]
        }
      },
      "authRoute": {
        "ClusterId": "authCluster",
        "Match": {
          "Path": "/auth-gate/{**catch-all}",
          "Methods": [ "POST" ]
        }
      },
      "userRoute": {
        "ClusterId": "userCluster",
        "Match": {
          "Path": "/user-gate/{**catch-all}",
          "Methods": [ "GET", "POST", "PUT", "DELETE" ]
        }
      }
    },
    "Clusters": {
      "cartCluster": {
        "Destinations": {
          "cartDestination": {
            "Address": "http://localhost:5259/api/"
          }
        }
      },
      "productCluster": {
        "Destinations": {
          "productDestination": {
            "Address": "http://localhost:5233/api/"
          }
        }
      },
      "authCluster": {
        "Destinations": {
          "authDestination": {
            "Address": "http://localhost:5216/api/"
          }
        }
      },
      "userCluster": {
        "Destinations": {
          "userDestination": {
            "Address": "http://localhost:5216/api/"
          }
        }
      }
    }
  }
}
