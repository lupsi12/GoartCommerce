{
  "ReverseProxy": {
    "Routes": {
      "cartRoute": {
        "ClusterId": "cartCluster",
        "Match": {
          "Path": "/cart/{**catch-all}",
          "Methods": [ "GET", "POST", "PUT", "DELETE" ]
        }
      },
      "campaignRoute": {
        "ClusterId": "campaignCluster",
        "Match": {
          "Path": "/campaign/{**catch-all}",
          "Methods": [ "GET", "POST", "PUT", "DELETE" ]
        }
      },
      "campaignProductRoute": {
        "ClusterId": "campaignProductCluster",
        "Match": {
          "Path": "/campaignProduct/{**catch-all}",
          "Methods": [ "GET", "POST", "PUT", "DELETE" ]
        }
      },
      "viewedProductRoute": {
        "ClusterId": "viewedProductCluster",
        "Match": {
          "Path": "/viewedProduct/{**catch-all}",
          "Methods": [ "GET", "POST", "PUT", "DELETE" ]
        }
      },
      "productRoute": {
        "ClusterId": "productCluster",
        "Match": {
          "Path": "/products/{**catch-all}",
          "Methods": [ "GET", "POST", "PUT", "DELETE" ]
        }
      },
      "authRoute": {
        "ClusterId": "authCluster",
        "Match": {
          "Path": "/auth/{**catch-all}",
          "Methods": [ "POST" ]
        }
      },
      "userRoute": {
        "ClusterId": "userCluster",
        "Match": {
          "Path": "/user/{**catch-all}",
          "Methods": [ "GET", "POST", "PUT", "DELETE" ]
        }
      }
    },
    "Clusters": {
      "cartCluster": {
        "Destinations": {
          "cartDestination": {
            "Address": "http://cartservice:8080/api/"
          }
        }
      },
      "campaignCluster": {
        "Destinations": {
          "campaignDestination": {
            "Address": "http://homeservice:8080/api/"
          }
        }
      },
      "campaignProductCluster": {
        "Destinations": {
          "campaignProductDestination": {
            "Address": "http://homeservice:8080/api/"
          }
        }
      },
      "viewedProductCluster": {
        "Destinations": {
          "viewedProductDestination": {
            "Address": "http://homeservice:8080/api/"
          }
        }
      },
      "productCluster": {
        "Destinations": {
          "productDestination": {
            "Address": "http://productservice:8080/api/"
          }
        }
      },
      "authCluster": {
        "Destinations": {
          "authDestination": {
            "Address": "http://authservice:8080/api/"
          }
        }
      },
      "userCluster": {
        "Destinations": {
          "userDestination": {
            "Address": "http://authservice:8080/api/"
          }
        }
      }
    }
  }
}
